
@{
    ViewBag.Title = "Races";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<body>
    <h2>Races</h2>

    <div id="myCarousel" class="carousel slide" data-ride="carousel" >

        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active" style="background-color:saddlebrown"></li>
            <li data-target="#myCarousel" data-slide-to="1" style="background-color:saddlebrown"></li>
            <li data-target="#myCarousel" data-slide-to="2" style="background-color:saddlebrown"></li>
            <li data-target="#myCarousel" data-slide-to="3" style="background-color:saddlebrown"></li>
            <li data-target="#myCarousel" data-slide-to="4" style="background-color:saddlebrown"></li>
            <li data-target="#myCarousel" data-slide-to="5" style="background-color:saddlebrown"></li>
            <li data-target="#myCarousel" data-slide-to="6" style="background-color:saddlebrown"></li>
            <li data-target="#myCarousel" data-slide-to="7" style="background-color:saddlebrown"></li>
            <li data-target="#myCarousel" data-slide-to="8" style="background-color:saddlebrown"></li>
        </ol>

        <div class="carousel-inner">
            <div class="item active">

                <img src="~/images/humain.png" alt="human" style="height:500px;position:center;">
                <div class="carousel-caption">
                    <h3 style="color:black">Human</h3>
                </div>
            </div>
            <div class="item">

                <img src="~/images/nain.png" alt="Dwarf" style="height:500px;position:center;">
                <div class="carousel-caption">
                    <h3 style="color:black">Dwarf</h3>
                </div>
            </div>
            <div class="item">

                <img src="~/images/elf.png" alt="Elf" style="height:500px;position:center;">
                <div class="carousel-caption">
                    <h3 style="color:black">Elf</h3>
                </div>
            </div>
            <div class="item">

                <img src="~/images/razzan.png" alt="Razzan" style="height:500px;position:center;">
                <div class="carousel-caption">
                    <h3 style="color:black">Razzan</h3>
                </div>
            </div>
            <div class="item">

                <img src="~/images/nekkan.png" alt="Nekkan" style="height:500px;position:center;">
                <div class="carousel-caption">
                    <h3 style="color:black">Nekkan</h3>
                </div>
            </div>
            <div class="item">

                <img src="~/images/Felkkan.png" alt="Felkkan" style="height:500px;position:center;">
                <div class="carousel-caption">
                    <h3 style="color:black">Felkkan</h3>
                </div>
            </div>
            <div class="item">

                <img src="~/images/ursian.png" alt="Ursian" style="height:500px;position:center;">
                <div class="carousel-caption">
                    <h3 style="color:black">Ursian</h3>
                </div>
            </div>

            <div class="item">

                <img src="~/images/okkan.png" alt="Okkan" style="height:500px;position:center;">
                <div class="carousel-caption">
                    <h3 style="color:black">Okkan</h3>
                </div>
            </div>

            <div class="item">
                <img src="~/images/bebokkan.png" alt="Bebokkan" style="height:500px">
                <div class="carousel-caption">
                    <h3 style="color:black">Bebokkan</h3>
                </div>
            </div>


        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>

    </div>
</body>
